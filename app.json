{
  "expo": {
    "name": "GroundFaultDetector",
    "slug": "ground-fault-detector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "groundfaultdetector",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.damarjati22.groundfaultdetector"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9cd093a1-c09e-4b1e-ac36-8dd6276e3ba6"
      }
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["react-native-chart-kit"],
        "listUnknownPackages": false
      }
    }
  }
}
